<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_marginBottom="@dimen/_20sdp"
    android:layout_height="match_parent">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hi Arun,How can we help you today?"
            android:fontFamily="@font/googlesans_regular"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginTop="@dimen/_10sdp"
        android:id="@+id/customerwish_name"/>
    <EditText
        android:id="@+id/searchEditText"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_35sdp"
        android:background="@drawable/edt_txt_bgg"
        android:layout_marginTop="@dimen/_10sdp"
        android:fontFamily="@font/googlesans_regular"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:drawableLeft="@drawable/ic_baseline_search_24"
        android:textSize="@dimen/_12sdp"
        android:hint="What would you like to shop today"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingLeft="@dimen/_10sdp"
        android:textColorHint="#7f7f7f"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_30sdp"
        android:text="View All Odrders"
        android:background="@drawable/edt_txt_bg"
        android:fontFamily="@font/googlesans_bold"
        android:textColor="#e61c72"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_gravity="center"
        android:drawableEnd="@drawable/ic_baseline_arrow_forward_12"
        android:gravity="center"
        android:layout_marginTop="@dimen/_10sdp"
        android:id="@+id/customerview_orders"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Regarding Orders"
                android:padding="@dimen/_10sdp"
                android:background="#f2f2f2"
                android:textSize="@dimen/_14sdp"
                android:fontFamily="@font/googlesans_medium"/>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_220sdp"
        app:cardElevation="10dp"
        android:layout_margin="@dimen/_10sdp"
        app:cardBackgroundColor="@color/white"
        app:cardMaxElevation="12dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:weightSum="3"
            android:orientation="horizontal">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical">
            <ImageSwitcher
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_50sdp"
                android:background="@drawable/image_circle">

                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/delivery_slot"
                    android:scaleType="fitXY"
                    android:layout_gravity="center"
                    android:gravity="center"
                    />
            </ImageSwitcher>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:fontFamily="@font/googlesans_medium"
                android:text="Change Delivery Slot"
                android:textColor="#000" />

        </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:orientation="vertical">
                <ImageSwitcher
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:background="@drawable/image_circle">
                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/cancel_orders"
                    android:layout_gravity="center"
                    android:scaleType="fitXY"
                    android:gravity="center" />
                </ImageSwitcher>
                <TextView
                    android:layout_width="@dimen/_60sdp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:fontFamily="@font/googlesans_medium"
                    android:text="Cancel Orders"
                    android:textColor="#000" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_weight="1"
                android:orientation="vertical">
                <ImageSwitcher
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:background="@drawable/image_circle">
                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/return_exachanges"
                    android:scaleType="fitXY"
                    android:layout_gravity="center"
                    android:gravity="center" />
                </ImageSwitcher>
                <TextView
                    android:layout_width="@dimen/_60sdp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:fontFamily="@font/googlesans_medium"
                    android:text="Return &amp; Exchanges"
                    android:textColor="#000" />

            </LinearLayout>

        </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
               android:layout_marginLeft="@dimen/_10sdp"
             android:layout_gravity="center"
                android:layout_marginTop="@dimen/_10sdp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="vertical">
                    <ImageSwitcher
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:background="@drawable/image_circle">
                    <ImageView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:src="@drawable/pay_order"
                        android:scaleType="fitXY"
                       android:layout_gravity="center"
                        android:gravity="center" />
                    </ImageSwitcher>
                    <TextView
                        android:layout_width="@dimen/_60sdp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:fontFamily="@font/googlesans_medium"
                        android:text="Pay for and Order"
                        android:textColor="#000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginLeft="@dimen/_35sdp"
                    android:orientation="vertical">
                    <ImageSwitcher
                        android:layout_width="@dimen/_50sdp"
                        android:layout_height="@dimen/_50sdp"
                        android:background="@drawable/image_circle">
                    <ImageView
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:src="@drawable/forgot_voucher"
                        android:scaleType="fitXY"
                        android:layout_gravity="center"
                        android:gravity="center" />
                    </ImageSwitcher>
                    <TextView
                        android:layout_width="@dimen/_60sdp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:fontFamily="@font/googlesans_medium"
                        android:text="Forgot eVoucher"
                        android:textColor="#000" />




                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Popular Searches"
                android:padding="@dimen/_10sdp"
                android:background="#f2f2f2"
                android:textSize="@dimen/_14sdp"
                android:fontFamily="@font/googlesans_medium"/>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardElevation="10dp"
        android:layout_marginBottom="@dimen/_20sdp"
        app:cardBackgroundColor="@color/white"
        app:cardMaxElevation="12dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Iam not happy with quality of product"
            android:textColor="#000"
            android:fontFamily="@font/googlesans_regular"
            android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:id="@+id/cnothappy"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:background="@android:color/darker_gray"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Iam unable to Login/Signup"
                android:textColor="#000"
                android:fontFamily="@font/googlesans_regular"
                android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:id="@+id/c_unabletologin"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:background="@android:color/darker_gray"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="I want to contact Customer Service"
                android:textColor="#000"
                android:fontFamily="@font/googlesans_regular"
                android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:id="@+id/c_customerservice"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:background="@android:color/darker_gray"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="I want to change the Delivery Slot"
                android:textColor="#000"
                android:fontFamily="@font/googlesans_regular"
                android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:id="@+id/c_changedelslot"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:background="@android:color/darker_gray"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="I want to add a product to my Existing Slot"
                android:textColor="#000"
                android:fontFamily="@font/googlesans_regular"
                android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:id="@+id/c_addexsitingslot"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:background="@android:color/darker_gray"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Chat with Us"
                android:textColor="#e61c72"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/googlesans_bold"
                android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_24"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_gravity="center"
                android:gravity="center"
                android:drawableTint="#e61c72"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:id="@+id/c_chatwithus"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_20sdp"
                android:background="@android:color/darker_gray"/>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
    </LinearLayout>
        </LinearLayout>

    </androidx.core.widget.NestedScrollView>
</LinearLayout>